<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="is.epn.cornershop">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".form_pay"
                android:parentActivityName=".metod_pay"></activity>
        <activity android:name=".metod_pay"
                android:parentActivityName=".car_shop"/>

        <activity
                android:name=".car_shop"
                android:label="@string/title_activity_car_shop"
                android:theme="@style/AppTheme.NoActionBar"
                android:parentActivityName=".catalogue"/>
        <activity
                android:name=".catalogue"
                android:label="@string/title_activity_catalogue"
                android:theme="@style/AppTheme.NoActionBar"/>



        <activity
                android:name=".MapsActivity"
                android:label="@string/title_activity_maps3">

        </activity>
        <activity android:name=".registrar">

        </activity>
        <meta-data
                android:name="com.google.android.geo.API_KEY"
                android:value="@string/google_maps_key" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>